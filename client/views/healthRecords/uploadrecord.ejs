<!DOCTYPE html>
<html lang="en">

<head>
  <title>上传记录</title>
  <% include ../layout/layout %>
<!--   <script src="<%=_src('/lib/vconsole.min.js')%>"></script> -->
  <script type="text/javascript">
    var share = new share("<%=_src('')%>") //jssdk分享页面
  </script>
  <style>
    body{
      background: #ebebeb;
      color: #666;
    }
    .uploadrecord{
      margin-bottom: 10px;
    }
    .weui-cells{
      margin-top: 0;
    }
    .flex{
      display: flex;
    }
    .time{
      padding: 0 10px;
    }
    .status{
      color: #f90;
    }
  </style>
</head>

<body ontouchstart>
  <% if (content.length == 0) { %>
    <div style="text-align: center;">
      没有数据
    </div>
  <% } else { %>
    <% for (var i = 0; i < content.length; i ++) { %>
      <div class="uploadrecord">
        <div class="weui-cells">
          <div class="weui-cell">
            <div class="weui-cell__hd"><img src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block"></div>
            <div class="weui-cell__bd flex">
              <div class="time"><%=content[i].updateDate%></div>
              <div class="status"><%=content[i].statusName%></div> 
            </div>
          </div>
        </div>
        <div class="weui-cells weui-cells_form" id="uploader">
           <div class="weui-cell">
               <div class="weui-cell__bd">
                   <div class="weui-uploader">
                       <div class="weui-uploader__bd">
                           <ul class="weui-uploader__files uploaderFiles">
                            <% for (var j = 0; j < content[i].imgUrlList.length; j ++) { %>
                              <li class="weui-uploader__file" style="background-image:url(<%=content[i].imgUrlList[j]%>)"></li>
                            <% } %>
                           </ul>                   
                       </div>
                   </div>
               </div>
           </div>
        </div>
      </div>
    <% } %>
  <% } %>
</body>
<script>
  // var vConsole = new VConsole();

$(document).ready(function(){
  $(".uploaderFiles").on('click','li', function() {
    var imgList = [];
    var src = $(this).css('background-image').split("\"")[1];
    imgList.push(src)
    $(this).siblings().each(function(index){
      imgList.push($(this).css('background-image').split("\"")[1])
    })
    console.log(imgList)
    console.log(src)
    if (imgList.length != 0) {
      wx.previewImage({
        current: src, // 当前显示图片的http链接
        urls: imgList // 需要预览的图片http链接列表
      });
    } else {
      console.log('没有照片');
    }
  })
})
  
</script>
</html>

<!DOCTYPE html>
<html lang="en">
<head>
  <title>查看随访计划</title>
  <% include ../layout/layout %>
  <script type="text/javascript">
    var share = new share("<%=_src('')%>") //jssdk分享页面
  </script>
  <style>
    body{
      background: #ebebeb;
      color: #666;
    }
    .weui-cells{
      margin-top: 0;
    }
    .cycle{
      margin-top: 10px;
      background: #fff;
    }
  </style>
</head>

<body ontouchstart>
  <div class="followup_plan_detail">
    <div class="follow_up_time">
      <div class="weui-cell">
        <div class="weui-cell__bd">
            <p>计划开始时间</p>
        </div>
        <div class="weui-cell__ft"><%=Y%>-<%=M%>-<%=D%></div>
      </div>
      <div class="weui-cells">
        <div class="weui-cell">
            <div class="weui-cell__hd"><img src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block"></div>
            <div class="weui-cell__bd">
                <p>随访周期</p>
            </div>
            <div class="weui-cell__ft">请您按计划复查项目</div>
        </div>
      </div>
    </div>
    <div class="follow_up_cycle">
      <% for (let i = 0; i < flupDetail.flupList.length; i++) { %>
        <div class="cycle">
          <div class="weui-cell">
              <% if (i === 0) {%>
                <div class="weui-cell__bd">
                  <p>一个月后</p>
                </div>
                <% if (M+1 > 12) {%>
                  <div class="weui-cell__ft"><%=Y+1%>-<%=1%>-<%=D%></div>
                <% } else {%>
                  <div class="weui-cell__ft"><%=Y%>-<%=M+1%>-<%=D%></div>
                <% } %>
              <% } %>
              <% if (i === 1) {%>
                <div class="weui-cell__bd">
                  <p>三个月后</p>
                </div>
                <% if (M+3 > 12) {%>
                  <div class="weui-cell__ft"><%=Y+1%>-<%=M-9%>-<%=D%></div>
                <% } else {%>
                  <div class="weui-cell__ft"><%=Y%>-<%=M+3%>-<%=D%></div>
                <% } %>
              <% } %>
              <% if (i === 2) {%>
                <div class="weui-cell__bd">
                  <p>六个月后</p>
                </div>
                <% if (M+6 > 12) {%>
                  <div class="weui-cell__ft"><%=Y+1%>-<%=M-6%>-<%=D%></div>
                <% } else {%>
                  <div class="weui-cell__ft"><%=Y%>-<%=M+6%>-<%=D%></div>
                <% } %>
              <% } %>
              <% if (i === 3) {%>
                <div class="weui-cell__bd">
                  <p>九个月后</p>
                </div>
                <% if (M+9 > 12) {%>
                  <div class="weui-cell__ft"><%=Y+1%>-<%=M-3%>-<%=D%></div>
                <% } else {%>
                  <div class="weui-cell__ft"><%=Y%>-<%=M+9%>-<%=D%></div>
                <% } %>
              <% } %>
              <% if (i === 4) {%>
                <div class="weui-cell__bd">
                  <p>十二个月后</p>
                </div>
                <div class="weui-cell__ft"><%=Y+1%>-<%=M%>-<%=D%></div>
              <% } %> 
          </div>
            <% for (let j = 0; j < flupDetail.flupList[i].length; j++) { %>
              <div class="weui-cell">
                  <div class="weui-cell__bd">
                      <p><%=flupDetail.flupList[i][j]%></p>
                  </div>
                  <div class="weui-cell__ft"></div>
              </div>
            <% } %>
        </div>
      <% } %>
    </div>
  </div>
</body>
<script>

$(document).ready(function(){
  
})
  
</script>
</html>
